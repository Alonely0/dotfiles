[Desktop Entry]
Encoding=UTF-8
Name=Timed Image Wallpaper
Keywords=
Icon=preferences-desktop-wallpaper

Type=Service

X-KDE-ServiceTypes=Plasma/Wallpaper
X-KDE-ParentApp=
X-KDE-PluginInfo-Name=com.darkeye.timedImage
X-KDE-PluginInfo-EnabledByDefault=true
X-KDE-PluginInfo-Version=3
X-KDE-PluginInfo-Website=https://github.com/dark-eye/com.darkeye.timedImage

X-Plasma-MainScript=ui/main.qml

MimeType=image/gif;image/png;image/svg+xml;image/svg+xml-compressed;
X-Plasma-DropMimeTypes=image/gif,image/png,image/svg+xml,image/svg+xml-compressed
